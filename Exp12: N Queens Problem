#include <stdio.h>
#include <stdlib.h>

int cnt=0;
void solve(int *b, int r, int n){
	if(r==n){cnt++;return;}
	for(int c=0; c<n; c++){
		int ok=1;
		for(int i=0; i<r; i++)
			if(b[i]==c||abs(b[i]-c)==abs(i-r)) ok=0;
		if(ok){b[r]=c;solve(b,r+1,n);}
	}
}
int main(){
	int n; scanf("%d",&n);
	int b[n]; solve(b,0,n);
	printf("%d\n",cnt);
}
